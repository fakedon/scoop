            },
            "arm64": {
                "url": "https://github.com/wiserain/rclone/releases/download/v$version/rclone-v$version-windows-arm64.zip",
                "extract_dir": "rclone-v$version-windows-arm64"
            }
        },
        "hash": {
            "url": "$baseurl/SHA256SUMS"
        }
    }
